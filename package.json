{
  "name": "abhisheks-shop",
  "version": "0.0.1",
  "description": "A Reaction Commerce plugin that creates swagshop.reactioncommerce.com",
  "main": "register.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/reactioncommerce/abhisheks-shop.git"
  },
  "keywords": [
    "reaction",
    "commerce",
    "examples",
    "plugins"
  ],
  "author": "Reaction Commerce",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/reactioncommerce/abhisheks-shop/issues"
  },
  "homepage": "https://github.com/reactioncommerce/abhisheks-shop#readme",
  "dependencies": {
    "buffer-stream-reader": "^0.1.1"
  },
  "babel": {
    "plugins": [
      [
        "lodash",
        {
          "id": [
            "lodash",
            "recompose"
          ]
        }
      ],
      [
        "module-resolver",
        {
          "root": [
            "./"
          ],
          "alias": {
            "@reactioncommerce/reaction-collections": "./imports/plugins/core/collections",
            "@reactioncommerce/reaction-components": "./imports/plugins/core/components/lib",
            "@reactioncommerce/reaction-router": "./imports/plugins/core/router/lib",
            "@reactioncommerce/reaction-ui": "./imports/plugins/core/ui/client/components",
            "underscore": "lodash"
          }
        }
      ]
    ],
    "presets": [
      "env",
      "react",
      "stage-2"
    ]
  },
  "eslintConfig": {
    "extends": "@reactioncommerce"
  },
  "devDependencies": {
    "eslint-import-resolver-meteor": "^0.4.0",
    "babel-eslint": "^8.2.1",
    "@reactioncommerce/eslint-config": "^1.0.1",
    "eslint": "^4.17.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.6.1",
    "eslint-plugin-jest": "^21.14.0"
  }
}
